{
  "name": "vite-plugin-sitemap",
  "version": "0.1.1",
  "packageManager": "pnpm@6.32.10",
  "description": "Sitemap generator plugin for Vite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "npm run build -- --watch",
    "example:build": "npm -C example run build",
    "release": "npx bumpp --commit --tag --push && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "fast-glob": "^3.2.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.21.1",
    "@antfu/utils": "0.5.1",
    "@types/node": "17.0.30",
    "c8": "7.11.2",
    "eslint": "8.14.0",
    "sitemap": "7.1.1",
    "tsup": "5.12.6",
    "typescript": "4.6.4",
    "vite": "2.9.6",
    "vitest": "0.10.0",
    "vue-tsc": "0.34.10",
    "xml-formatter": "2.6.1"
  }
}
